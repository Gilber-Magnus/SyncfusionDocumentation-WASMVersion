<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="compilerconfig.json" />
    <Content Remove="Pages\Component.razor" />
  </ItemGroup>

  <ItemGroup>
    <None Include="compilerconfig.json" />
    <None Include="Pages\Component.razor" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="7.0.13" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="7.0.13" PrivateAssets="all" />
    <PackageReference Include="Npm" Version="3.5.2" />
    <PackageReference Include="Syncfusion.Blazor.buttons" Version="23.1.42" />
    <PackageReference Include="Syncfusion.Blazor.Calendars" Version="23.1.42" />
    <PackageReference Include="Syncfusion.Blazor.Core" Version="23.1.42" />
    <PackageReference Include="Syncfusion.Blazor.Inputs" Version="23.1.42" />
    <PackageReference Include="syncfusion.blazor.lists" Version="23.1.42" />
    <PackageReference Include="Syncfusion.Blazor.Popups" Version="23.1.42" />
    <PackageReference Include="Syncfusion.Blazor.Themes" Version="23.1.42" />
  </ItemGroup>

  <ItemGroup>
    <UpToDateCheckInput Remove="Pages\Component.razor" />
  </ItemGroup>

  <ItemGroup>
    <_ContentIncludedByDefault Remove="Pages\Component.razor" />
  </ItemGroup>

</Project>
